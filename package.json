{
  "name": "ntglogin",
  "version": "1.0.0",
  "description": "GoLogin clone backend with Node.js + TypeScript + Express + Prisma",
  "main": "dist/index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.ts\""
  },
  "keywords": [
    "gologin",
    "browser",
    "profile",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@types/puppeteer": "^5.4.7",
    "argon2": "^0.44.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "electron": "^28.0.0",
    "express": "^5.1.0",
    "get-port": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "otpauth": "^9.4.1",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer": "^24.27.0",
    "puppeteer-core": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "random-useragent": "^0.5.0",
    "socket.io": "^4.8.1",
    "socks-proxy-agent": "^8.0.5",
    "user-agents": "^1.1.669",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/argon2": "^0.14.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
