{
  "name": "ntg-login-tool",
  "version": "1.0.2",
  "main": "main.js",
  "description": "Tool nuôi nick & Edit video tự động",
  "author": "NTG Team",
  "scripts": {
    "dev": "concurrently \"npm run build:backend && node backend/dist/index.js\" \"vite\" \"electron .\"",
    "web": "concurrently \"node backend/dist/index.js\" \"vite\"",
    "build:react": "vite build",
    "build:backend": "npx tsc -p ./tsconfig.json --outDir ./backend/dist",
    "dist": "npm run build:react && npm run build:backend && electron-builder"
  },
  "build": {
    "npmRebuild": false,
    "appId": "com.ntg.login",
    "productName": "NTG Login System",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "backend/dist/**/*",
      "electron.js",
      "package.json",
      "prisma/schema.prisma"
    ],
    "extraResources": [
      {
        "from": "prisma/data.db",
        "to": "prisma/data.db"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "public/laptop.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    }
  },
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-table": "^8.21.3",
    "@types/puppeteer": "^5.4.7",
    "argon2": "^0.44.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.2",
    "get-port": "^7.1.0",
    "get-video-duration": "^5.0.0",
    "https-proxy-agent": "^7.0.6",
    "jimp": "^1.6.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "morgan": "^1.10.0",
    "otpauth": "^9.4.1",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer": "^24.27.0",
    "puppeteer-core": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "random-useragent": "^0.5.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.68.0",
    "react-nice-avatar": "^1.5.0",
    "react-router-dom": "^7.10.1",
    "reactflow": "^11.11.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socks-proxy-agent": "^8.0.5",
    "tailwind-merge": "^3.4.0",
    "user-agents": "^1.1.669",
